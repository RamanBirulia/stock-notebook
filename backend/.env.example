# Database Configuration
DATABASE_URL=sqlite:stocks.db

# Alpha Vantage API Configuration
ALPHA_VANTAGE_API_KEY=your_alpha_vantage_api_key_here

# JWT Configuration
JWT_SECRET=your_jwt_secret_here

# API Logging Configuration
# Set to "true" to enable detailed logging of API requests to Alpha Vantage
ENABLE_API_REQUEST_LOGGING=false

# Set to "true" to enable detailed logging of API responses from Alpha Vantage
# Warning: This will log full API responses which can be very verbose
ENABLE_API_RESPONSE_LOGGING=false

# Rust Log Level (optional)
# Uncomment and set to control overall logging level
# RUST_LOG=info,stock_tracker_backend=debug
